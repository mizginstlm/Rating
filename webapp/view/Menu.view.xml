<mvc:View
    busy="{viewModel>/isbusy}"
    controllerName="mozos.rating.controller.Menu"
    xmlns:semantic="sap.m.semantic"
    xmlns:layout="sap.ui.layout"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
    xmlns:f="sap.f"
    xmlns:card="sap.f.cards"
>
    <semantic:FullscreenPage id="page2" floatingFooter="true"
        title="{globalviewModel>/currentLogin/Ename} - {globalviewModel>/currentLogin/Pltxt}">
        <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
            header="{createTravelRequest}" subheader="   " press="requestTilePress">
            <tileContent>
                <TileContent unit="" footer="">
                    <content>
                        <ImageContent src="sap-icon://travel-itinerary" />
                    </content>
                </TileContent>
            </tileContent>
        </GenericTile>
        <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
            header="{myTravelRequests}" subheader="   " press="myRequestsTilePress">
            <tileContent>
                <TileContent unit="" footer="">
                    <content>
                        <ImageContent src="sap-icon://activity-items" />
                    </content>
                </TileContent>
            </tileContent>
        </GenericTile>
    </semantic:FullscreenPage>
</mvc:View>