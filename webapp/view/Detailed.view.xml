		<VBox>
			<Panel headerText="Emekdaşın Sınaq Mueddeti Deyerlendirilmesi">
				<VBox class="sapUiSmallMargin" alignItems="Center">
					<!-- <Title id="helpcommission1" text="{i18n>helpinfo}" /> -->
					<Table class="showList" id="showList"
						mode="SingleSelectMaster"
						busy="{main>/busy}"
						selectionChange="handleSelectionChange"
						items="{
					path: 'viewModel>/validatorData'
				}"
						growing="true"
						busyIndicatorDelay="0"
						width="auto"
					>
						<headerToolbar />
						<columns>
							<Column id="_IDGenColumn2" vAlign="Middle" hAlign="Begin" width="6rem">
								<Text id="_IDGenTex2" class="sapThemeFont" text="Maddeler" />
							</Column>
							<Column id="_IDGenColumn1" vAlign="Middle" width="16rem">
								<Text id="_IDGenText" class="sapThemeFont" text="Açıklama" />
							</Column>
							<Column id="_IDGenColumn5" vAlign="Middle" hAlign="End" width="6rem">
								<Text id="_IDGenText5" class="sapThemeFont" text="Değerlendirme" />
							</Column>
						</columns>
						<items>
							<ColumnListItem vAlign="Middle" id="entitlementColumnListItem">
								<cells>
									<ObjectAttribute id="_IDGenObjectAttribute4"
										text="{viewModel>sqmtx}" />
									<ObjectAttribute id="_IDGenObjectAttribute1"
										text="{viewModel>sqltx}" />
									<RatingIndicator id="RI_L" maxValue="10"
										class="sapUiSmallMarginBottom"
										value="{viewModel>rate}" iconSize="12px" enabled="true"
										editable="false" tooltip="Rating Tooltip" />
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</VBox>
			</Panel>
		</VBox>
		<HBox>
			<Panel headerText="Dəyərləndirme Sonucu">
				<HBox>
					<VBox width="50%">

						<Table class="showList" id="showList1"
							mode="SingleSelectMaster"
							busy="{main>/busy}"
							selectionChange="handleSelectionChange"
							growing="true"
							busyIndicatorDelay="0"
							width="auto"
						>
							<headerToolbar />
							<columns>
								<Column id="_IDGenColumn12" vAlign="Middle" hAlign="Begin"
									width="6rem">
									<Text id="_IDGenTex21" class="sapThemeFont" text="Yekun"
										textAlign="Center" />
								</Column>
								<Column id="_IDGenColumn11" vAlign="Middle" width="6rem">
									<Text id="_IDGenTex1t" class="sapThemeFont" text="Netice"
										textAlign="Center" />
								</Column>

							</columns>
							<items>
								<ColumnListItem vAlign="Middle" id="entitlementColumnListItem1">
									<cells>
										<ObjectAttribute id="_IDGenObjectAttribute14"
											text="{viewModel>/questionDatas/total}" />

										<ObjectAttribute id="_IDGenObjectAttribute11"
											text="{viewModel>/questionDatas/sqrst}" />

									</cells>
								</ColumnListItem>
							</items>
						</Table>


					</VBox>
					<!-- class="sapUiLargeMargin" -->
					<HBox alignContent="Center" alignItems="Center" justifyContent="Center"
						width="50%">
						<VBox width="50%">
							<l:VerticalLayout class="sapUiContentPadding">
								<MessageStrip
									text="nəticə 0-20 : Sınaq müddətində uyğun görülməmişdir"
									type="Information">
								</MessageStrip>
								<MessageStrip
									text="nəticə 21-40 : Sınaq müddətini keçmişdir"
									type="Warning"
									close="handleClose">
								</MessageStrip>
								<MessageStrip
									text="nəticə 41-50 : Sınaq müddətini uğurla keçmişdir"
									type="Success">
								</MessageStrip>
							</l:VerticalLayout>
						</VBox>
					</HBox>


				</HBox>
			</Panel>
		</HBox>


		<VBox id="managementdecision" class="sapUiSmallMargin">
			<Panel expandable="{main>/managementDecision}"
				visible="{main>/managementDecisioniVisible}"
				headerText="Müəssisə rəhbərinin təsdiq ve qeydləri">
				<f:Form id="FormDisplay480_12120Dual8" editable="false">
					<f:title>
						<!-- <core:Title id="helpcommission1" text="{i18n>helpinfo}" /> -->
